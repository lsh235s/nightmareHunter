    leftCharacter: Richard
    rightCharacter: 
    characterAnimation: idle2_talk
  - stage_id: 0
    event_stage_id: 2
    content: "\uC774\uD574\uD574\uC918. \uC790\uBCF8\uC0AC\uD68C\uB77C\uB2C8\uAE50.
